{
  "name": "@jpyc/sdk-core",
  "version": "2.0.0",
  "license": "MIT",
  "description": "Node SDK for interacting with JPYC contracts",
  "keywords": [
    "jpyc",
    "jpy",
    "stablecoin",
    "ethereum",
    "evm",
    "sdk",
    "typescript",
    "node",
    "viem"
  ],
  "repository": "https://github.com/jcam1/sdks.git",
  "homepage": "https://github.com/jcam1/sdks.git#readme",
  "bugs": {
    "url": "https://github.com/jcam1/sdks/issues/new?labels=bug,sdk-core&template=bug_report.md"
  },
  "engines": {
    "node": "^20.12.0",
    "yarn": "^1.22.22"
  },
  "type": "module",
  "main": "dist/src",
  "types": "dist/types/src",
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "compile": "tsc",
    "test": "vitest run",
    "lint": "eslint --fix ./src",
    "lint:dry-run": "eslint ./src",
    "format": "prettier --write ./src",
    "format:dry-run": "prettier --check ./src",
    "docs": "run-p docs:md docs:html",
    "docs:md": "typedoc --plugin typedoc-plugin-markdown --out ../../docs/core/md",
    "docs:html": "typedoc --out ../../docs/core/html"
  },
  "dependencies": {
    "soltypes": "^2.0.0",
    "viem": "^2.38.5"
  },
  "devDependencies": {
    "@eslint/js": "^9.38.0",
    "@types/node": "^24.9.2",
    "@typescript-eslint/eslint-plugin": "^8.46.2",
    "@typescript-eslint/parser": "^8.46.2",
    "eslint": "^9.38.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.6.2",
    "ts-node": "^10.9.2",
    "tslib": "^2.8.1",
    "typedoc": "^0.28.14",
    "typedoc-plugin-markdown": "^4.9.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.46.2",
    "vitest": "^4.0.6"
  }
}
